{
  "type": "plugin",
  "name": "OneAI Tagger",
  "handle": "oneai-tagger",
  "version": "0.0.3",
  "entrypoint": "api.handler",
  "description": "Tags text using the OneAI API.",
  "author": "Steamship",
  "public": true,
  "configTemplate": {
    "apikey": {
      "type": "string"
    },
    "input_type": {
      "type": "string",
      "default": "article"
    },
    "skills": {
      "type": "string",
      "default": "entities"
    }
  },
  "plugin": {
    "type": "tagger",
    "path": "/tag",
    "isTrainable": false,
    "transport": "jsonOverHttp",
    "runPath": "/run"
  }
}